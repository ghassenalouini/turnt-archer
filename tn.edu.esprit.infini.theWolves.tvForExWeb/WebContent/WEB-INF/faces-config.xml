<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	<navigation-rule>
		<display-name>WelcomeTvForex.xhtml</display-name>
		<from-view-id>/WelcomeTvForex.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>navtoLogin</from-outcome>
			<to-view-id>/Pages/Platfrom/Login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Pages/Platfrom/Login.xhtml</display-name>
		<from-view-id>/Pages/Platfrom/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>navtoregister</from-outcome>
			<to-view-id>/Pages/Platfrom/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
